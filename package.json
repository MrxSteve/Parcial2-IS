{
  "name": "parcial-docker-integrado",
  "version": "1.0.0",
  "description": "API de gestión de mascotas - Parcial Docker con PostgreSQL - Arquitectura MVC",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "dev:watch": "node --watch server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "docker:build": "docker build -t parcial-api .",
    "docker:run": "docker run -d -p 3000:3000 --name parcial-container parcial-api",
    "docker:stop": "docker stop parcial-container",
    "docker:logs": "docker logs parcial-container",
    "docker:clean": "docker stop parcial-container && docker rm parcial-container"
  },
  "keywords": [
    "nodejs",
    "express",
    "docker",
    "postgresql",
    "mascotas",
    "api",
    "mvc",
    "rest",
    "crud"
  ],
  "author": "Tu Nombre Aquí",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {},
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
